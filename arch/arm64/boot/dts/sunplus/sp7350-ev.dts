	thermal: thermal@f88002b4 {
		compatible = "sunplus,sp7350-thermal";
		reg =<0x0 0xf88002b4 0x0 0x18>;
		clocks = <&clkc THERMAL>;
		resets = <&rstc RST_THERMAL>;
		#thermal-sensor-cells = <0>;
		nvmem-cell-names = "therm_calib";
		nvmem-cells = <&therm_calib>;
		//status = "disabled";
	};

	thermal-zones {
		cpu_thermal_zone: cpu-thermal {
			polling-delay-passive = <5000>;
			polling-delay = <1000>;

			thermal-sensors = <&thermal 0>;
			sustainable-power = <100000>;

			passive = <80>;
			trips {
					cpu_hott: cpu-hott {
						temperature = <120000>;
						hysteresis  = <1000>;
						type = "hot";
					};
					cpu_crit: cpu-crit {
						temperature = <125000>;
						hysteresis  = <0>;
						type = "critical";
					};
					cpu_trip0: cpu0 {
						temperature = <70000>;
						hysteresis = <2000>;
						type = "passive";
					};
					cpu_trip1: cpu1 {
						temperature = <80000>;
						hysteresis = <2000>;
						type = "active";
				};
			};
		};
	};

