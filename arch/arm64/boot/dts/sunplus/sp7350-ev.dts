			hdmi-timing = <0x2>;
			#endif
			/*
			 * 0x0: 720x480
			 * 0x2: 1280x720
			 * 0x3: 1920x1080
			 */
			status = "okay";
			ports {
				#address-cells = <1>;
				#size-cells = <0>;
				port@0 {
					reg = <0>;
					lt8912b_in: endpoint {
						remote-endpoint = <&disp_phy>;
						data-lanes = <1 2 3 4>;
					};
				};

				port@1 {
					reg = <1>;
					lt8912b_out: endpoint {
						remote-endpoint = <&hdmi_con_out>;
					};
				};
			};
		};
#endif
	};

