	ahbdma1: dma@f8821000 {
		compatible = "snps,dma-spi";
		reg =<0x0 0xf8821000 0x0 0x400>;
		interrupt-parent = <&gic>;
		interrupts = <GIC_SPI 178 IRQ_TYPE_EDGE_RISING>,
			     <GIC_SPI 180 IRQ_TYPE_EDGE_RISING>,
			     <GIC_SPI 192 IRQ_TYPE_EDGE_RISING>,
			     <GIC_SPI 193 IRQ_TYPE_EDGE_RISING>,
			     <GIC_SPI 194 IRQ_TYPE_EDGE_RISING>,
			     <GIC_SPI 195 IRQ_TYPE_EDGE_RISING>,
			     <GIC_SPI 196 IRQ_TYPE_EDGE_RISING>,
			     <GIC_SPI 199 IRQ_TYPE_EDGE_RISING>;
		dma-channels = <8>;
		#dma-cells = <3>;
		dma-requests = <16>;
		chan_allocation_order = <1>;
		chan_priority = <1>;
		block_size = <0xfff>;
		dma-masters = <4>;
		data-width = <2 2 2 2>;
		multi-block = <0 0 0 0 0 0 0 0>;
		snps,max-burst-len = <128 128 128 128 128 128 128 128>;
		status = "disabled";
	};

	spi_controller0: spi@f8822000 {
		compatible = "snps,dw-apb-ssi";
		reg =<0x0 0xf8822000 0x0 0x100>;
		#address-cells = <1>;
		#size-cells = <0>;
		interrupt-parent = <&gic>;
		interrupts = <GIC_SPI 27 IRQ_TYPE_LEVEL_HIGH>;
		clocks = <&clkc SPICB0>;
		resets = <&rstc RST_SPICB0>;
#ifndef SPI_LOOPBACK_MTTEST
		pinctrl-names = "default";
		pinctrl-0 = <&spi_master0_pins>;
		dmas = <&ahbdma1 0 0 0>,
		       <&ahbdma1 1 0 0>;
		dma-names = "rx", "tx";
		status = "disabled";
#endif
		spi@0 {
			compatible = "rohm,dh2228fv";
			spi-max-frequency = <5000000>;
			reg = <0x0>;
#ifndef SPI_LOOPBACK_MTTEST
			status = "disabled";
#endif
