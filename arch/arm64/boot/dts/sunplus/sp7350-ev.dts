			<&rstc RST_GPOST2>, <&rstc RST_GPOST3>, <&rstc RST_IMGREAD0>, <&rstc RST_MIPITX>,
			<&rstc RST_OSD0>, <&rstc RST_OSD1>, <&rstc RST_OSD2>, <&rstc RST_OSD3>,
			<&rstc RST_TCON>, <&rstc RST_TGEN>, <&rstc RST_VPOST0>, <&rstc RST_VSCL0>;
		reset-names =
			"rstc_dispsys", "rstc_dmix", "rstc_gpost0", "rstc_gpost1",
			"rstc_gpost2", "rstc_gpost3", "rstc_imgread0", "rstc_mipitx",
			"rstc_osd0", "rstc_osd1", "rstc_osd2", "rstc_osd3",
			"rstc_tcon", "rstc_tgen", "rstc_vpost0", "rstc_vscl0";
		status = "okay";
#if defined(DISP_MIPITX_CSI)
	#if defined(DISP_MIPITX_CSI_480P)
		sp7350,dev_name = "MIPITX-CSI-TEST_480P";
		sp7350,osd0_layer = <0 0 720 480 0x4>; /* 0x4 = YUY2 */
		sp7350,vpp0_layer = <0 0 720 480 720 480 0x2>; /* 0x2 = YUY2 */
		sp7350,disp_output = <720 480 0x1>; /* 0x1:MIPITX CSI */
	#elif defined(DISP_MIPITX_CSI_720P)
		sp7350,dev_name = "MIPITX-CSI-TEST_720P";
		sp7350,osd0_layer = <0 0 1280 720 0x4>; /* 0x4 = YUY2 */
		sp7350,vpp0_layer = <0 0 1280 720 1280 720 0x2>; /* 0x2 = YUY2 */
		sp7350,disp_output = <1280 720 0x1>; /* 0x1:MIPITX CSI */
	#elif defined(DISP_MIPITX_CSI_1080P)
		sp7350,dev_name = "MIPITX-CSI-TEST_1080P";
		sp7350,osd0_layer = <0 0 1920 1080 0x4>; /* 0x4 = YUY2 */
		sp7350,vpp0_layer = <0 0 1920 1080 1920 1080 0x2>; /* 0x2 = YUY2 */
		sp7350,disp_output = <1920 1080 0x1>; /* 0x1:MIPITX CSI */
	#elif defined(DISP_MIPITX_CSI_3840)
		sp7350,dev_name = "MIPITX-CSI-TEST_3840";
		sp7350,osd0_layer = <0 0 3840 2880 0x4>; /* 0x4 = YUY2 */
		sp7350,vpp0_layer = <0 0 3840 2880 3840 2880 0x2>; /* 0x2 = YUY2 */
		sp7350,disp_output = <3840 2880 0x1>; /* 0x1:MIPITX CSI */
	#else
		sp7350,dev_name = "MIPITX-CSI-TEST_720P";
		sp7350,osd0_layer = <0 0 1280 720 0x4>; /* 0x4 = YUY2 */
		sp7350,vpp0_layer = <0 0 1280 720 1280 720 0x2>; /* 0x2 = YUY2 */
		sp7350,disp_output = <1280 720 0x1>; /* 0x1:MIPITX CSI */
	#endif
		sp7350,disp_mipitx_lane = <0x4>; /* 0x0:MIPITX lane */
		sp7350,disp_mipitx_clk_edge = <0x0>; /* 0x0:Raising Edge */
		sp7350,disp_mipitx_sync_timing = <0x1>; /* 0x1:Sync Event */
		sp7350,disp_mipitx_format = <0x1>; /* 0x1:MIPITX format CSI_16BITS YUY2 */

#elif defined(DISP_MIPITX_DSI_PANEL_240X320)
		sp7350,dev_name = "TCXD024IBLON-2";

		sp7350,osd0_layer = <0 0 240 320 0xe>; /* 0xe = ARGB8888 */
		//sp7350,osd1_layer = <0 0 240 320 0xe>; /* 0xe = ARGB8888 */
		//sp7350,osd2_layer = <0 0 240 320 0xe>; /* 0xe = ARGB8888 */
		//sp7350,osd3_layer = <0 0 240 320 0xe>; /* 0xe = ARGB8888 */
		sp7350,vpp0_layer = <0 0 720 480 240 320 0x2>; /* 0x2 = YUY2 */

		sp7350,disp_output = <240 320 0x0>; /* 0x0:MIPITX DSI */
		sp7350,disp_mipitx_lane = <0x1>; /* 0x0:MIPITX lane */
		sp7350,disp_mipitx_clk_edge = <0x1>; /* 0x1:Falling Edge */
		sp7350,disp_mipitx_sync_timing = <0x0>; /* 0x1:Sync Pulse */
		sp7350,disp_mipitx_format = <0x3>; /* 0x3:MIPITX DSI RGB888 */
#elif defined(DISP_MIPITX_DSI_PANEL_480X1280)
		//reset-gpios = <&pctl 56 GPIO_ACTIVE_HIGH>;
		sp7350,dev_name = "HXM0686TFT-001";
		sp7350,osd0_layer = <0 0 480 1280 0xe>; /* 0xe = ARGB8888 */
		sp7350,osd1_layer = <0 240 480 1280 0x2>; /* 0x2 = 8bpp */
		sp7350,osd2_layer = <0 480 480 1280 0x2>; /* 0x2 = 8bpp */
		sp7350,osd3_layer = <0 720 480 1280 0x2>; /* 0x2 = 8bpp */

		//sp7350,osd0_layer = <0 0 200 200 0xe>; /* 0xe = ARGB8888 */
		//sp7350,osd1_layer = <280 0 200 200 0xe>; /* 0xe = ARGB8888 */
		//sp7350,osd2_layer = <0 1080 200 200 0xe>; /* 0xe = ARGB8888 */
		//sp7350,osd3_layer = <280 1080 200 200 0xe>; /* 0xe = ARGB8888 */
			/* 0x2 = 8bpp (ARGB)
			 * 0x4 = YUY2
			 * 0x8 = RGB565
			 * 0x9 = ARGB1555
			 * 0xa = RGBA4444
			 * 0xb = ARGB4444
			 * 0xd = RGBA8888
			 * 0xe = ARGB8888
			 */
	#if defined(DISP_MIPITX_DSI_VPP_SRC_480P)
		sp7350,vpp0_layer = <0 0 720 480 480 1280 0x2>; /* 0x2 = YUY2 */
	#elif defined(DISP_MIPITX_DSI_VPP_SRC_720P)
		sp7350,vpp0_layer = <0 0 1280 720 480 1280 0x2>; /* 0x2 = YUY2 */
	#elif defined(DISP_MIPITX_DSI_VPP_SRC_1080P)
		sp7350,vpp0_layer = <0 0 1920 1080 480 1280 0x2>; /* 0x2 = YUY2 */
	#elif defined(DISP_MIPITX_DSI_VPP_SRC_3840)
		sp7350,vpp0_layer = <0 0 3840 2880 480 1280 0x2>; /* 0x2 = YUY2 */
	#else
		sp7350,vpp0_layer = <0 0 720 480 480 1280 0x2>; /* 0x2 = YUY2 */
			/* 0x1 = UYVY
			 * 0x2 = YUY2
			 * 0x3 = NV16
			 * 0x6 = NV24
			 * 0x7 = NV12
			 * else YUY2
			 */
	#endif

		sp7350,disp_output = <480 1280 0x0>; /* 0x0:MIPITX DSI */
			/* 0x0:MIPITX DSI
			 * 0x1:MIPITX CSI
			 */
		sp7350,disp_mipitx_lane = <0x4>; /* 0x0:MIPITX lane */
			/* 0x1 = 1 lane
			 * 0x2 = 2 lane
			 * 0x4 = 4 lane
			 */
		sp7350,disp_mipitx_clk_edge = <0x1>; /* 0x1:Falling Edge */
			/* 0x0:Raising Edge
			 * 0x1:Falling Edge
			 */
		sp7350,disp_mipitx_sync_timing = <0x1>; /* 0x1:Sync Event */
			/* 0x0:Sync Pulse
			 * 0x1:Sync Event
			 */
		sp7350,disp_mipitx_format = <0x3>; /* 0x3:MIPITX DSI RGB888 */
			/* 0x0 = DSI RGB565        / CSI_16BITS RGB565
			 * 0x1 = DSI RGB666_18BITS / CSI_16BITS YUY2 8bit
			 * 0x2 = DSI RGB666_24BITS / CSI_20BITS YUY2 10bit
			 * 0x3 = DSI RGB888        / CSI_24BITS RGB888
			 */
#elif defined(RASPBERRYPI_DSI_PANEL) /* Add raspberrypi_lcd subnode */
		sp7350,dev_name = "RASPBERRYPI_DSI_PANEL";
		sp7350,osd0_layer = <0 0 800 480 0x8>; /* 0x8 = RGB565 */
		sp7350,osd1_layer = <0 0 800 480 0x8>; /* 0x8 = RGB565 */
		sp7350,osd2_layer = <0 0 800 480 0x8>; /* 0x8 = RGB565 */
		sp7350,osd3_layer = <0 0 800 480 0x8>; /* 0x8 = RGB565 */
		sp7350,vpp0_layer = <0 0 720 480 800 480 0x2>; /* 0x2 = YUY2 */
		sp7350,disp_output = <800 480 0x0>; /* 0x0:MIPITX DSI */
		sp7350,disp_mipitx_lane = <0x1>; /* 0x0:MIPITX lane */
		sp7350,disp_mipitx_clk_edge = <0x0>; /* 0x0:Raising Edge */
		sp7350,disp_mipitx_sync_timing = <0x0>; /* 0x0:Sync Pulse */
		sp7350,disp_mipitx_format = <0x3>; /* 0x3:MIPITX DSI RGB888 */
		port {
			disp_phy: endpoint {
				remote-endpoint = <&panel_dsi_port>;
			};
		};

#elif defined(LT8912B_HDMI_ENABLE) /* Add lt8912b subnode */
		sp7350,dev_name = "LT8912B-HDMI";
	#if defined(LT8912B_HDMI_480X272)
		sp7350,osd0_layer = <0 0 480 272 0x8>; /* 0x8 = RGB565 */
		sp7350,vpp0_layer = <0 0 720 480 480 272 0x2>; /* 0x2 = YUY2 */
		sp7350,disp_output = <720 480 0x0>; /* 0x0:MIPITX DSI */
	#elif defined(LT8912B_HDMI_480P)
		sp7350,osd0_layer = <0 0 720 480 0x8>; /* 0x8 = RGB565 */
		sp7350,osd1_layer = <0 0 720 480 0x8>; /* 0x8 = RGB565 */
		sp7350,osd2_layer = <0 0 720 480 0x8>; /* 0x8 = RGB565 */
		sp7350,osd3_layer = <0 0 720 480 0x8>; /* 0x8 = RGB565 */
		sp7350,vpp0_layer = <0 0 720 480 720 480 0x2>; /* 0x2 = YUY2 */
		sp7350,disp_output = <720 480 0x0>; /* 0x0:MIPITX DSI */
	#elif defined(LT8912B_HDMI_720P)
		sp7350,osd0_layer = <0 0 1280 720 0x8>; /* 0x8 = RGB565 */
		sp7350,osd1_layer = <0 0 1280 720 0x8>; /* 0x8 = RGB565 */
		sp7350,osd2_layer = <0 0 1280 720 0x8>; /* 0x8 = RGB565 */
		sp7350,osd3_layer = <0 0 1280 720 0x8>; /* 0x8 = RGB565 */
		sp7350,vpp0_layer = <0 0 720 480 1280 720 0x2>; /* 0x2 = YUY2 */
		sp7350,disp_output = <1280 720 0x0>; /* 0x0:MIPITX DSI */
	#elif defined(LT8912B_HDMI_1080P)
		sp7350,osd0_layer = <0 0 1920 1080 0x8>; /* 0x8 = RGB565 */
		sp7350,osd1_layer = <0 0 1920 1080 0x8>; /* 0x8 = RGB565 */
		sp7350,osd2_layer = <0 0 1920 1080 0x8>; /* 0x8 = RGB565 */
		sp7350,osd3_layer = <0 0 1920 1080 0x8>; /* 0x8 = RGB565 */
		sp7350,vpp0_layer = <0 0 720 480 1920 1080 0x2>; /* 0x2 = YUY2 */
		sp7350,disp_output = <1920 1080 0x0>; /* 0x0:MIPITX DSI */
	#else
		sp7350,osd0_layer = <0 0 1280 720 0x8>; /* 0x8 = RGB565 */
		sp7350,osd1_layer = <0 0 1280 720 0x8>; /* 0x8 = RGB565 */
		sp7350,osd2_layer = <0 0 1280 720 0x8>; /* 0x8 = RGB565 */
