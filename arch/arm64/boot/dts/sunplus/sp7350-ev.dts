		sp7350,disp_output = <1280 720 0x0>; /* 0x0:MIPITX DSI */
	#endif
		sp7350,disp_mipitx_lane = <0x4>; /* 0x0:MIPITX lane */
		sp7350,disp_mipitx_clk_edge = <0x0>; /* 0x1:Raising Edge */
