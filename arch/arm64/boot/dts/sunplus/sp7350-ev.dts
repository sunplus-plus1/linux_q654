	i2c6: i2c@f882e000 {
		compatible = "snps,designware-i2c";
		reg =<0x0 0xf882e000 0x0 0x100>;
		interrupt-parent = <&gic>;
		interrupts = <GIC_SPI 22 IRQ_TYPE_LEVEL_HIGH>;
		clocks = <&clkc I2CM6>;
		resets = <&rstc RST_I2CM6>;
#if defined(LT8912B_HDMI_ENABLE) || defined(RASPBERRYPI_DSI_PANEL)
		/* fix setting for lt8912b & raspberrypi_lcd */
		//i2c-scl-rising-time-ns = <0>;
		//i2c-scl-falling-time-ns = <0>;
		i2c-sda-hold-time-ns = <2000>;
		clock-frequency = <100000>;
#else
		//i2c-sda-hold-time-ns = <1000>;
		//clock-frequency = <400000>;
#endif
		pinctrl-names = "default";
		pinctrl-0 = <&i2c_combo6_pins>;
		status = "okay";

#ifdef RASPBERRYPI_DSI_PANEL /* Add raspberrypi_lcd subnode in i2c6 */
		#address-cells = <1>;
		#size-cells = <0>;

		raspberrypi_lcd: lcd@45 {
			//compatible = "raspberrypi,7inch-touchscreen-panel";
			compatible = "raspberrypi,7inch-touchscreen-panel-builtin";
			reg = <0x45>;
			port {
				panel_dsi_port: endpoint {
				remote-endpoint = <&disp_phy>;
				};
			};
		};
#endif
#ifdef LT8912B_HDMI_ENABLE /* Add lt8912b subnode in i2c6 */
		#address-cells = <1>;
		#size-cells = <0>;

		lontium_lt8912b: hdmi@48 {
			compatible = "lontium,lt8912b";
			//compatible = "lontium,lt8912b-builtin";
			reg = <0x48>;
			//reset-gpios = <&pctl 74 GPIO_ACTIVE_LOW>;
			#if defined(LT8912B_HDMI_480P) || defined(LT8912B_HDMI_480X272)
			hdmi-timing = <0x0>;
			#elif defined(LT8912B_HDMI_720P)
			hdmi-timing = <0x2>;
			#elif defined(LT8912B_HDMI_1080P)
			hdmi-timing = <0x3>;
			#else
			hdmi-timing = <0x2>;
			#endif
			/*
			 * 0x0: 720x480
			 * 0x2: 1280x720
			 * 0x3: 1920x1080
			 */
			status = "okay";
			ports {
				#address-cells = <1>;
				#size-cells = <0>;
				port@0 {
					reg = <0>;
					lt8912b_in: endpoint {
						remote-endpoint = <&disp_phy>;
						data-lanes = <1 2 3 4>;
					};
				};

				port@1 {
					reg = <1>;
					lt8912b_out: endpoint {
						remote-endpoint = <&hdmi_con_out>;
					};
				};
			};
		};
#endif
	};

