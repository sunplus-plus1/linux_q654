		sp7350,osd0_layer = <0 0 1280 720 0x4>; /* 0x4 = YUY2 */
		sp7350,vpp0_layer = <0 0 1280 720 1280 720 0x2>; /* 0x2 = YUY2 */
		sp7350,disp_output = <1280 720 0x1>; /* 0x1:MIPITX CSI */
	#endif
		sp7350,disp_mipitx_lane = <0x4>; /* 0x0:MIPITX lane */
		sp7350,disp_mipitx_clk_edge = <0x0>; /* 0x0:Raising Edge */
		sp7350,disp_mipitx_sync_timing = <0x1>; /* 0x1:Sync Event */
		sp7350,disp_mipitx_format = <0x1>; /* 0x1:MIPITX format CSI_16BITS YUY2 */

#elif defined(DISP_MIPITX_DSI_PANEL_240X320)
		sp7350,dev_name = "TCXD024IBLON-2";

		sp7350,osd0_layer = <0 0 240 320 0xe>; /* 0xe = ARGB8888 */
		//sp7350,osd1_layer = <0 0 240 320 0xe>; /* 0xe = ARGB8888 */
		//sp7350,osd2_layer = <0 0 240 320 0xe>; /* 0xe = ARGB8888 */
		//sp7350,osd3_layer = <0 0 240 320 0xe>; /* 0xe = ARGB8888 */
		sp7350,vpp0_layer = <0 0 720 480 720 480 0x2>; /* 0x2 = YUY2 */

		sp7350,disp_output = <240 320 0x0>; /* 0x0:MIPITX DSI */
		sp7350,disp_mipitx_lane = <0x1>; /* 0x0:MIPITX lane */
		sp7350,disp_mipitx_clk_edge = <0x1>; /* 0x1:Falling Edge */
		sp7350,disp_mipitx_sync_timing = <0x0>; /* 0x1:Sync Pulse */
		sp7350,disp_mipitx_format = <0x3>; /* 0x3:MIPITX DSI RGB888 */
#elif defined(DISP_MIPITX_DSI_PANEL_480X1280)
		//reset-gpios = <&pctl 56 GPIO_ACTIVE_HIGH>;
		sp7350,dev_name = "HXM0686TFT-001";
		sp7350,osd0_layer = <0 0 480 1280 0xe>; /* 0xe = ARGB8888 */
		sp7350,osd1_layer = <0 240 480 1280 0x2>; /* 0x2 = 8bpp */
		sp7350,osd2_layer = <0 480 480 1280 0x2>; /* 0x2 = 8bpp */
		sp7350,osd3_layer = <0 720 480 1280 0x2>; /* 0x2 = 8bpp */

		//sp7350,osd0_layer = <0 0 200 200 0xe>; /* 0xe = ARGB8888 */
		//sp7350,osd1_layer = <280 0 200 200 0xe>; /* 0xe = ARGB8888 */
		//sp7350,osd2_layer = <0 1080 200 200 0xe>; /* 0xe = ARGB8888 */
		//sp7350,osd3_layer = <280 1080 200 200 0xe>; /* 0xe = ARGB8888 */
			/* 0x2 = 8bpp (ARGB)
			 * 0x4 = YUY2
			 * 0x8 = RGB565
			 * 0x9 = ARGB1555
			 * 0xa = RGBA4444
			 * 0xb = ARGB4444
			 * 0xd = RGBA8888
			 * 0xe = ARGB8888
			 */
	#if defined(DISP_MIPITX_DSI_VPP_SRC_480P)
		sp7350,vpp0_layer = <0 0 720 480 720 480 0x2>; /* 0x2 = YUY2 */
	#elif defined(DISP_MIPITX_DSI_VPP_SRC_720P)
		sp7350,vpp0_layer = <0 0 1280 720 1280 720 0x2>; /* 0x2 = YUY2 */
	#elif defined(DISP_MIPITX_DSI_VPP_SRC_1080P)
		sp7350,vpp0_layer = <0 0 1920 1080 1920 1080 0x2>; /* 0x2 = YUY2 */
	#elif defined(DISP_MIPITX_DSI_VPP_SRC_3840)
		sp7350,vpp0_layer = <0 0 3840 2880 3840 2880 0x2>; /* 0x2 = YUY2 */
	#else
		sp7350,vpp0_layer = <0 0 720 480 720 480 0x2>; /* 0x2 = YUY2 */
			/* 0x1 = UYVY
			 * 0x2 = YUY2
			 * 0x3 = NV16
			 * 0x6 = NV24
			 * 0x7 = NV12
			 * else YUY2
			 */
	#endif

		sp7350,disp_output = <480 1280 0x0>; /* 0x0:MIPITX DSI */
			/* 0x0:MIPITX DSI
			 * 0x1:MIPITX CSI
			 */
		sp7350,disp_mipitx_lane = <0x4>; /* 0x0:MIPITX lane */
			/* 0x1 = 1 lane
			 * 0x2 = 2 lane
			 * 0x4 = 4 lane
			 */
		sp7350,disp_mipitx_clk_edge = <0x1>; /* 0x1:Falling Edge */
